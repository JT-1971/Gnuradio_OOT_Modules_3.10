        <type>void</type>
        <definition>void read_and_correct_hex_word</definition>
        <argsstring>(dsd_opts *opts, dsd_state *state, char *hex, int *status_count, AnalogSignal *analog_signal_array, int *analog_signal_index)</argsstring>
        <name>read_and_correct_hex_word</name>
        <param>
          <type><ref refid="structdsd__opts" kindref="compound">dsd_opts</ref> *</type>
          <declname>opts</declname>
        </param>
        <param>
          <type><ref refid="structdsd__state" kindref="compound">dsd_state</ref> *</type>
          <declname>state</declname>
        </param>
        <param>
          <type>char *</type>
