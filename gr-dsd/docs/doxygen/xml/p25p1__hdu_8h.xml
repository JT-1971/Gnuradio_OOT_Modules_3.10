        <definition>void read_dibit_update_analog_data</definition>
        <argsstring>(dsd_opts *opts, dsd_state *state, char *buffer, unsigned int count, int *status_count, AnalogSignal *analog_signal_array, int *analog_signal_index)</argsstring>
        <name>read_dibit_update_analog_data</name>
        <param>
          <type><ref refid="structdsd__opts" kindref="compound">dsd_opts</ref> *</type>
          <declname>opts</declname>
        </param>
        <param>
          <type><ref refid="structdsd__state" kindref="compound">dsd_state</ref> *</type>
